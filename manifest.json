  {
    "name": "FILONYA Productivity Task Stack",
    "version": "0.0.1",
    "description": "Tool for managing open Chrome tabs using FILO stack approach",
    "permissions": ["activeTab", "storage"],
    "background": {
      "scripts": ["app.js", "background.js"],
      "persistent": false
    },
    "commands": {
      "new-context": {
        "suggested_key": {
          "default": "Alt+T"
        },
        "description": "New Tab + Filonya Context"
      }
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "64": "stack-64.png"
      }
    },
    "manifest_version": 2
  }
