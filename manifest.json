  {
    "name": "FILONYA Productivity Task Stack",
    "version": "0.0.1",
    "default_locale": "en",
    "description": "Tool for managing open Chrome tabs using FILO stack approach",
    "permissions": ["activeTab", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "64": "stack-64.png"
      }
    },
    "manifest_version": 2
  }
